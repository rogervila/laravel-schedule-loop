{
    "name": "rogervila/laravel-schedule-loop",
    "description": "schedule loop command for Laravel that runs schedule every N seconds (60 by default)",
    "keywords": [
        "laravel",
        "schedule run",
        "run",
        "loop",
        "schedule",
        "scheduler"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Roger VilÃ ",
            "email": "rogervila@me.com"
        }
    ],
    "require": {
        "php": ">=7.2",
        "laravel/framework": "5.8.*|^6.0|^7.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.14",
        "phpunit/phpunit": "^7.5 || ^8.0 || ^9.0",
        "orchestra/testbench": "^3|^4|^5"
    },
    "autoload": {
        "psr-4": {
            "ScheduleLoop\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ScheduleLoop\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ScheduleLoop\\ScheduleLoopServiceProvider"
            ]
        }
    }
}
